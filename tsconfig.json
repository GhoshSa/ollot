{
	"compilerOptions": {
		"jsx": "react",
		"target": "es6",
		"module": "commonjs",
		"rootDir": "src",
		"outDir": "out",
		"esModuleInterop": true,
		"lib": [
			"es6",
			"dom",
			"dom.iterable"
		],
		"sourceMap": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": [
		"src/**/*",
		"src/typings/*"
	],
	"exclude": [
		"node_modules",
		"out"
	]
}
{
  "name": "ollot",
  "displayName": "Ollot",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "title": "chat with your ollama model",
        "command": "ollot.openChat"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ollot-sidebar",
          "title": "Ollot",
          "icon": "src/icon/chat.svg"
        }
      ]
    },
    "views": {
      "ollot-sidebar": [
        {
          "type": "webview",
          "id": "ollot.chatView",
          "name": "Chat",
          "visibility": "visible"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "ollot.openChat",
          "when": "view == ollot.chatView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.97.0",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "@typescript-eslint/parser": "^8.22.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.19.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "axios": "^1.7.9"
  },
  "files": [
    "out/**/*"
  ]
}
